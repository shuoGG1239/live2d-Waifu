{
  "name": "liv2d-waifu-electron",
  "version": "0.1.0",
  "description": "纸片人老婆(electron)",
  "author": "shuoGG",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "liv2d-waifu",
    "appId": "com.shuogg.live2dwaifu",
    "directories": {
      "output": "build"
    },
    "files": [
      "assets/",
      "resources/",
      "dist/",
      "node_modules/",
      "index.html",
      "main.js",
      "package.json"
    ],
    "win": {
      "icon": "Assets/icon.ico"
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    }
  },
  "devDependencies": {
    "electron": "^7.0.0",
    "electron-builder": "^21.2.0"
  },
  "private": true
}
